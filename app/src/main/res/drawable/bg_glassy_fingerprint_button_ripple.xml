<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#60A8E6">
    
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:type="linear"
                android:angle="135"
                android:startColor="#4088C7"
                android:endColor="#2068A8" />
            <stroke
                android:width="1dp"
                android:color="#60A8E6" />
            <corners android:radius="16dp" />
        </shape>
    </item>
    
</ripple>
